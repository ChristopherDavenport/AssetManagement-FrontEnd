# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /assetTest                  controllers.HomeController.assetTest(in)
#GET     /assetAgreement             controllers.HomeController.assetAgreement


GET     /Agreement                  controllers.HomeController.Agreement
GET     /AssetAgreement             controllers.HomeController.AssetAgreement
GET     /Building                   controllers.HomeController.Building
GET     /Group                      controllers.HomeController.Group
GET     /IP                         controllers.HomeController.IP

GET     /Manufacturer               controllers.HomeController.Manufacturer
GET     /Master                     controllers.HomeController.Master
GET     /Note                       controllers.HomeController.Note
GET     /NoteType                   controllers.HomeController.NoteType
GET     /Owner                      controllers.HomeController.Owner

GET     /OwnerGroup                 controllers.HomeController.OwnerGroup
GET     /Printer                    controllers.HomeController.Printer
GET     /ServiceType                controllers.HomeController.ServiceType
GET     /Type                       controllers.HomeController.Type
GET     /Vendor                     controllers.HomeController.Vendor
